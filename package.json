{
  "scripts": {
    "start": "npm-run-all --parallel browsersync tailwindcss",
    "browsersync": "browser-sync . --files './index.html, ./src/output.css'",
    "tailwindcss": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "test": "vnu --skip-non-html --Werror _site/"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "daisyui": "^2.42.1"
  }
}
